{
  "name": "yaadfeed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node build-with-ignore.js",
    "build:analyze": "ANALYZE=true next build",
    "check-missing-images": "node scripts/check-missing-images.js",
    "clear-and-regenerate": "node scripts/clear-and-regenerate.js",
    "clear-and-regenerate:20": "node scripts/clear-and-regenerate.js --count=20",
    "clear-and-regenerate:30": "node scripts/clear-and-regenerate.js --count=30",
    "create-placeholders": "node scripts/create-placeholder-images.js",
    "dev": "next dev -p 4000",
    "export": "npm run build && mkdir -p ./out && cp -r .next/standalone/* ./out/ && cp -r .next/static ./out/.next/ && cp -r public ./out/",
    "fix-images": "node scripts/fix-article-images.js",
    "lint": "next lint",
    "regenerate": "node scripts/regenerate-articles.js",
    "regenerate:15": "node scripts/regenerate-articles.js --count=15",
    "regenerate:help": "node scripts/regenerate-articles.js --help",
    "regenerate:images": "node scripts/regenerate-articles.js --mode=update-images",
    "scrape-artists": "node scripts/scrape-all-artists.js",
    "scrape-w-artists": "ts-node scripts/scrape-w-artists.ts",
    "seed-admin": "node scripts/seed-admin-user.js",
    "start": "next start",
    "test-scraping": "node scripts/test-scraping-with-images.js",
    "test:mongodb": "node scripts/test-mongodb-connection.js"
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.1.3",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.18",
    "framer-motion": "^12.23.15",
    "lucide-react": "^0.544.0",
    "mongodb": "^5.9.2",
    "next": "^15.5.3",
    "next-auth": "^4.24.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-spinners": "^0.17.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/http-cache-semantics": "^4.0.4",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/webidl-conversions": "^7.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "^15.5.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.8.3"
  }
}
